{
  "name": "hivemind-node-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon --exec ts-node --files src/index.ts",
    "start:production": "pm2 start --name 'backend' node -- -r ./tsconfig-paths.js build/index.js",
    "build": "cd src && tsc"
  },
  "dependencies": {
    "axios": "^0.19.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-validator": "^6.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.1",
    "node-schedule": "^1.3.2",
    "nodemon": "^2.0.2",
    "pg": "^7.17.1",
    "prettier": "^1.19.1",
    "save": "^2.4.0",
    "socket.io": "^2.3.0",
    "url-metadata": "^2.4.0",
    "winston": "^3.2.1"
